OCAML_LIBS = $(OCAML_LIBS_nylas-api)

FILES[] =
  nylas_json
  nylas_api_t
  nylas_api_j
  nylas_api_v
  nylas_time
  nylas_filter
  nylas_multipart
  nylas_app
  nylas_api

Atdgen(nylas_api, -j-std)
OCamlLibrary(nylas-api, $(FILES))
Meta(nylas-api)

.DEFAULT: META nylas-api.cma nylas-api.cmxa

.PHONY: clean
clean:
  rm -f *.o *.a *.cm* *~ *.annot *.run *.opt META $(ATDGEN_OUTFILES)
